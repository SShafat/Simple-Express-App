        <div>
            
            <table border="1">
                <th> Name </th>
                <th> Age </th>
                
                <tr>
                    
                    
                    <td> <%= name %> </td>
                    <td> <%= age %> </td>
                </tr>
                
            </table>    
    
        </div>
    
    </body>

</html>